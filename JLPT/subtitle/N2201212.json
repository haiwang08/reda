        {
            "st": 1536.3200000000002,
            "et": 1539.32,
            "text": "1.会社の売り上げ目標"
        },
        { "st": 1542.32, "et": 1545.48, "text": "2.新製品の特徴" },
        { "st": 1548.1, "et": 1551.7, "text": "3.新製品の企画の募集" },
        { "st": 1554.56, "et": 1566.96, "text": "4.アルバイトの募集" },
        {
            "st": 1567.76,
