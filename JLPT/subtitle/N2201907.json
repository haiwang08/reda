        { "st": 444.71, "et": 445.03, "text": "え?" },
        { "st": 445.91, "et": 447.69, "text": "明日何かあるんですか?" },
        {
            "st": 448.55,
            "et": 451.37,
            "text": "すみません、掲示板ちゃんと読んでなくて。"
        },
        {
            "st": 451.69,
