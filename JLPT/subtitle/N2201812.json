        {
            "st": 1178.21,
            "et": 1180.21,
            "text": "ここでちょっと休みましょう。"
        },
        { "st": 1213.15, "et": 1215.01, "text": "では、また続けます。" },
        {
            "st": 1221.06,
            "et": 1227.58,
            "text": "問題3 問題3では問題用紙に何も印刷されていません。"
