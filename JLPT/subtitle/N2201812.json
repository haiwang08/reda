        {
            "st": 1605.37,
            "et": 1608.25,
            "text": "レポーターは何について話していますか?"
        },
        { "st": 1610.21, "et": 1614.11, "text": "1.ヤギの餌となる草の育て方" },
        {
            "st": 1616.8899999999999,
            "et": 1620.61,
